sleep 10 & pid1=$!
sleep 20 & pid2=$!
start=$(date "+%s")
sleep 1
kill $pid1 $pid2
wait
stop=$(date "+%s")
elapsed=$((stop - start))
echo $start - $stop = $elapsed
[ $((stop - start)) -lt 2 ] || exit 1

set -m
sleep 10 & pid1=$!
sleep 20 & pid2=$!
start=$(date "+%s")
sleep 1
kill %1 %2
wait
stop=$(date "+%s")
elapsed=$((stop - start))
echo $start - $stop = $elapsed
[ $((stop - start)) -lt 2 ] || exit 2
